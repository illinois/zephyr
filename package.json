{
  "name": "zephyr",
  "version": "1.0.0",
  "description": "A grading platform for CS 225",
  "bin": {
    "zephyr": "./lib/student-cli.js",
    "zephyr-staff": "./lib/staff-cli.js"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "build": "babel src --out-dir lib --verbose --extensions \".js,.ts,.tsx\"",
    "build:watch": "npm run build -- --watch",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nwalters512/zephyr.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/nwalters512/zephyr/issues"
  },
  "homepage": "https://github.com/nwalters512/zephyr#readme",
  "dependencies": {
    "@octokit/rest": "^15.9.4",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "csv-stringify": "^3.1.1",
    "deasync": "^0.1.13",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "fs-extra": "^6.0.1",
    "jest": "^23.4.0",
    "jest-plugin-fs": "^2.9.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10",
    "merge-trees": "^2.0.0",
    "moment": "^2.22.2",
    "ora": "^2.1.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.2.2",
    "simpl-schema": "^1.5.0",
    "tmp-promise": "^1.0.5",
    "validator": "^10.4.0",
    "xml2js": "^0.4.19",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@types/deasync": "^0.1.0",
    "@types/fs-extra": "^5.0.4",
    "@types/handlebars": "^4.0.38",
    "@types/jest": "^23.3.0",
    "@types/js-yaml": "^3.11.2",
    "@types/ora": "^1.3.4",
    "@types/request-promise-native": "^1.0.15",
    "@types/tmp": "0.0.33",
    "@types/validator": "^9.4.1",
    "eslint": "^5.1.0",
    "ts-jest": "^23.0.1",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
