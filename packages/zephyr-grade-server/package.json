{
  "name": "@illinois/zephyr-grade-server",
  "version": "1.0.0",
  "description": "A microservice to grade student code from HTTP requests",
  "main": "./lib/index.js",
  "scripts": {
    "start": "micro",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix-lint": "npm run lint -- --fix",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir lib --copy-files --verbose --extensions \".js,.ts,.tsx\"",
    "build:watch": "npm run build -- --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "illinois",
    "autograder"
  ],
  "author": "Nathan Walters",
  "license": "NCSA",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@types/node": "^10.5.4",
    "jest": "^23.4.1",
    "jest-plugin-fs": "^2.9.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.54",
    "@illinois/zephyr-catch-grader": "^1.0.3",
    "@illinois/zephyr-staff-cli": "^1.0.3",
    "@octokit/rest": "^15.9.4",
    "babel-polyfill": "^6.26.0",
    "debug": "^3.1.0",
    "micro": "^9.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
