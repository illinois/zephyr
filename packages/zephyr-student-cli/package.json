{
  "name": "@illinois/zephyr-student-cli",
  "version": "1.0.0",
  "description": "A CLI for zephyr for use by students",
  "bin": {
    "zephyr": "./lib/index.js"
  },
  "files":[
    "./lib"
  ],
  "scripts": {
    "test": "jest --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix-lint": "npm run lint -- --fix",
    "build": "babel src --out-dir lib --copy-files --verbose --extensions \".js,.ts,.tsx\"",
    "build:watch": "npm run build -- --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "illinois",
    "autograder"
  ],
  "author": "Nathan Walters",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.54",
    "@illinois/zephyr-catch-grader": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "ora": "^3.0.0",
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@types/jest": "^23.3.0",
    "@types/ora": "^1.3.4",
    "jest": "^23.4.1",
    "jest-plugin-fs": "^2.9.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
