{
  "name": "@illinois/zephyr-github-checkout",
  "version": "1.0.3",
  "description": "Module to do efficient checkouts using GitHub's API",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jest --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix-lint": "npm run lint -- --fix",
    "build": "tsc --emitDeclarationOnly && babel src --out-dir lib --copy-files --verbose --extensions \".js,.ts,.tsx\"",
    "build:watch": "npm run build -- --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "illinois",
    "autograder"
  ],
  "author": "Nathan Walters",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-typescript": "^7.0.0-beta.54",
    "@types/debug": "0.0.30",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.0",
    "@types/node": "^10.5.2",
    "@types/request-promise-native": "^1.0.15",
    "jest": "^23.4.1",
    "jest-plugin-fs": "^2.9.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.54",
    "@octokit/rest": "^15.9.4",
    "babel-polyfill": "^6.26.0",
    "debug": "^3.1.0",
    "fs-extra": "^7.0.0",
    "moment": "^2.22.2",
    "request-promise-native": "^1.0.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
